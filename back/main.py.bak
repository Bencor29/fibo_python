from typing import Optional
from fastapi import FastAPI
app = FastAPI()

def calculateFibonacciSum(n) :
    if (n <= 0) :
        return 0
  
    fibo =[0] * (n+1)
    fibo[1] = 1
  
    # Initialize result
    sn = fibo[0] + fibo[1]
  
    # Add remaining terms
    for i in range(2,n+1) :
        fibo[i] = fibo[i-1] + fibo[i-2]
        sn = sn + fibo[i]
    return sn

@app.get("/")
def read_root():
    return {"Hello": "World"}


@app.get("/fibo/{n}")
def read_item(n: int, q: Optional[str] = None):
    return {"n": n, "rslt": calculateFibonacciSum(n)}